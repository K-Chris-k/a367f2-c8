    "custom_liquid_ma3yWD": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<style>\n        table {\n            table-layout: fixed;\n            width: 100%;\n            margin-bottom: 10px;\n \n    }\n\n    .diytable{\n       /* border: 0.1rem solid rgba(var(--color-foreground),.2); */\n       border-collapse: collapse;\n    }\n \n\n        td {\n            \n        border: 0.1rem solid rgba(var(--color-foreground),.2);\n        text-align: center; /* 默认所有单元格居中 */\n        padding: 30px;\n        /* border: 1px solid #ddd; */\n        font-size: 16px;\n        font-family: 'Inter';\n        line-height: 19px;\n        font-weight: 400;\n        word-wrap: break-word;\n        color: #262626;\n        }\n\n        .diytable img{\n            width:150px;\n            /* vertical-align:center; */\n        }\n\n        td:first-child {\n     \n        text-align: left; /* 第一列靠左 */\n        font-family: 'Inter';\n        font-size: 18px;\n        font-weight: 500;\n        line-height: 22px;\n        border-left: none !important;\n        }\n\n       td:last-child {\n        border-right:  none !important;\n        }\n\n        tr:first-child td{\n            border-top:  none !important;\n            font-family: 'Inter';\n            font-size: 18px;\n            font-weight: 600;\n            line-height: 22px;\n            color: #3250C3;\n        }\n\n\n        @media screen and (max-width: 760px)  {\n            td {\n                text-align: center; /* 默认所有单元格居中 */\n                padding: 20px 8px 20px 8px;\n                border: 1px solid #ddd;\n                font-size: 12px;\n                font-family: 'Inter';\n                line-height: 15px;\n                font-weight: 400;\n                color: #262626;\n            }\n            \n            .diytable img{\n                width:90px;\n                vertical-align:center;\n            }\n\n            td:first-child {\n                width:35%;\n                text-align: left; /* 第一列靠左 */\n                font-family: 'Inter';\n                font-size: 12px;\n                font-weight: 500;\n                line-height: 15px;\n            }\n\n\n        tr:first-child td{\n            font-family: 'Inter';\n            font-size: 12px;\n            font-weight: 600;\n            line-height: 15px;\n            color: #3250C3;\n        }\n        } \n \n        .hidden-row {\n            display: none;\n        }\n        .controls_pc {\n            margin-bottom: 10px;\n        }\n        button {\n            padding: 5px 10px;\n            cursor: pointer;\n        }\n\n        .controls_pc a{\n            font-size: 16px;\n            font-family: 'Inter';\n            line-height: 19px;\n            font-weight: 400;\n            color: #3250C3;\n        }\n\n        /* 为带有特定类的td设置背景图片 */\n            td.with-image {\n            background-image: url('https://cdn.shopify.com/s/files/1/0596/6367/9587/files/Union.svg?v=1753958300');\n            \n            background-position: center;\n            background-repeat: no-repeat;\n            background-size: 15px 11px; /* 使图片匹配单元格尺寸 */\n            width:15px;\n            height:11px;\n            }\n            \n \n \n    </style>\n \n\n<div id=\"vergleich\">\n    <table id=\"myTable\" class=\"diytable \">\n            <tr style=\"border-top: none;\">\n                <td style=\"  border-collapse: collapse;\n  border: 1px solid #ddd; \n  border-top: none;       \n  border-left: none;      \n  border-right: none; width: 43.3%;\"></td>\n                <td style=\"width: 33.3%;\"><img src=\"https://cdn.shopify.com/s/files/1/0596/6367/9587/files/3_1.png?v=1753941892\" style=\"padding-bottom:20px;\" /><br>SolidFlex 2000</td>\n                <td style=\"width: 33.3%;\"><a href=\"/products/powerflex-2000-balkonkraftwerk-mit-speicher\"  ><img src=\"https://cdn.shopify.com/s/files/1/0596/6367/9587/files/3_1.png?v=1753941892\" style=\"padding-bottom:20px;\"/><br>PowerFlex 2000</a></td>\n            </tr>\n            <tr>\n        