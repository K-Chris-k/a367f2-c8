    "bes_product_tabs_xdrz6L": {
      "type": "bes-product-tabs",
      "blocks": {
        "tab_n9bUez": {
          "type": "tab",
          "settings": {
            "content_color_scheme": "",
            "navigation_title": "Plug & Play",
            "image": "shopify://shop_images/bes-tab1-pc.jpg",
            "mobile_image": "shopify://shop_images/bes-tab1-mob.jpg",
            "heading": "Einfache Installation. Sofort einsatzbereit.",
            "heading_size": "h1",
            "heading_highlight_style": "none",
            "subheading": "",
            "description": "<p>Mit Indevolt brauchst du keinen Elektriker. Die Installation ist unkompliziert im DIY-Stil – schließe einfach das AC-Kabel des Geräts an eine herkömmliche Steckdose zu Hause an und genieße eine einfache Handhabung ohne spezielles Fachwissen.</p>",
            "text_size": "text-base",
            "text_alignment": "center",
            "button_label": "Button label",
            "button_link": "",
            "button_style": "btn--primary"
