    "custom_liquid_ma3yWD": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<style>\n        table {\n            table-layout: fixed;\n            width: 100%;\n            margin-bottom: 10px;\n \n    }\n\n    .diytable{\n       /* border: 0.1rem solid rgba(var(--color-foreground),.2); */\n       border-collapse: collapse;\n    }\n \n\n        td {\n            \n        border: 0.1rem solid rgba(var(--color-foreground),.2);\n        text-align: center; /* 默认所有单元格居中 */\n        padding: 30px;\n        /* border: 1px solid #ddd; */\n        font-size: 16px;\n        font-family: 'Inter';\n        line-height: 19px;\n        font-weight: 400;\n        word-wrap: break-word;\n        color: #262626;\n        }\n\n        .diytable img{\n            width:150px;\n            /* vertical-align:center; */\n        }\n\n        td:first-child {\n     \n        text-align: left; /* 第一列靠左 */\n        font-family: 'Inter';\n        font-size: 18px;\n        font-weight: 500;\n        line-height: 22px;\n        border-left: none !important;\n        }\n\n       td:last-child {\n        border-right:  none !important;\n        }\n\n        tr:first-child td{\n            border-top:  none !important;\n            font-family: 'Inter';\n            font-size: 18px;\n            font-weight: 600;\n            line-height: 22px;\n            color: #3250C3;\n        }\n\n\n        @media screen and (max-width: 760px)  {\n            td {\n                text-align: center; /* 默认所有单元格居中 */\n                padding: 20px 8px 20px 8px;\n                border: 1px solid #ddd;\n                font-size: 12px;\n                font-family: 'Inter';\n                line-height: 15px;\n                font-weight: 400;\n                color: #262626;\n            }\n            \n            .diytable img{\n                width:90px;\n                vertical-align:center;\n            }\n\n            td:first-child {\n                width:35%;\n                text-align: left; /* 第一列靠左 */\n                font-family: 'Inter';\n                font-size: 12px;\n                font-weight: 500;\n                line-height: 15px;\n            }\n\n\n        tr:first-child td{\n            font-family: 'Inter';\n            font-size: 12px;\n            font-weight: 600;\n            line-height: 15px;\n            color: #3250C3;\n        }\n        } \n \n        .hidden-row {\n            display: none;\n        }\n        .controls_pc {\n            margin-bottom: 10px;\n        }\n        button {\n            padding: 5px 10px;\n            cursor: pointer;\n        }\n\n        .controls_pc a{\n            font-size: 16px;\n            font-family: 'Inter';\n            line-height: 19px;\n            font-weight: 400;\n            color: #3250C3;\n        }\n\n        /* 为带有特定类的td设置背景图片 */\n            td.with-image {\n            background-image: url('https://cdn.shopify.com/s/files/1/0596/6367/9587/files/Union.svg?v=1753958300');\n            \n            background-position: center;\n            background-repeat: no-repeat;\n            background-size: 15px 11px; /* 使图片匹配单元格尺寸 */\n            width:15px;\n            height:11px;\n            }\n            \n \n \n    </style>\n \n\n<div id=\"vergleich\">\n    <table id=\"myTable\" class=\"diytable \">\n            <tr style=\"border-top: none;\">\n                <td style=\"  border-collapse: collapse;\n  border: 1px solid #ddd; \n  border-top: none;       \n  border-left: none;      \n  border-right: none; width: 43.3%;\"></td>\n                <td style=\"width: 33.3%;\"><img src=\"https://cdn.shopify.com/s/files/1/0596/6367/9587/files/3_1.png?v=1753941892\" style=\"padding-bottom:20px;\" /><br>SolidFlex 2000</td>\n                <td style=\"width: 33.3%;\"><a href=\"/products/powerflex-2000-balkonkraftwerk-mit-speicher\"  ><img src=\"https://cdn.shopify.com/s/files/1/0596/6367/9587/files/3_1.png?v=1753941892\" style=\"padding-bottom:20px;\"/><br>PowerFlex 2000</a></td>\n            </tr>\n            <tr>\n                <td>Akkutyp</td>\n                <td>Halbfeste LiFePO₄</td>\n                <td>LiFePO₄</td>\n            </tr>\n            <tr>\n                <td>Kapazität</td>\n                <td>1792 Wh</td>\n                <td>2010 Wh</td>\n            </tr>\n            <tr >\n                <td>Lebenszyklen</td>\n                <td>8000</td>\n                <td>7000</td>\n            </tr>\n             <tr>\n                <td>PV-Eingang</td>\n                <td>2400W（je 600w）</td>\n                <td>2400W（je 600w）</td>\n            </tr>\n            <tr >\n                <td>Anzahl der MPPTS</td>\n                <td>4</td>\n                <td>4</td>\n            </tr>\n                        <tr >\n                <td>AC-Eingangs-/ Ausgangsleistung</td>\n                <td>2400W (mit Erweiterungsakkus)</td>\n                <td>2400W (mit Erweiterungsakkus)</td>\n            </tr>\n                        <tr >\n                <td>Kabellose Verbindung</td>\n                <td>Bluetooth, 2.4GHz Wi-Fi, 4G (Optional)</td>\n                <td>Bluetooth, 2.4GHz Wi-Fi, 4G (Optional)</td>\n            </tr>\n                        <tr >\n                <td>Wasserdichte Bewertung</td>\n                <td>IP65</td>\n                <td>IP65</td>\n            </tr>\n                        <tr >\n                <td>Einfacher Aufbau (Plug & Play)</td>\n                <td class=\"with-image\"> </td>\n                <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Akkuheizfunktion</td>\n                <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>App Steuerung</td>\n                <td class=\"with-image\"> </td>\n                <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Smart Meter für Nulleinspeisung</td>\n               <td class=\"with-image\"> </td>\n                <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Smart Plugs</td>\n               <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Bidirektionales Laden</td>\n               <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Zeitsteuerung</td>\n               <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Dynamische Stromtarife</td>\n               <td class=\"with-image\"> </td>\n                <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Notstromfunktion bei Stromausfall</td>\n               <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>EPS (10ms)</td>\n               <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>Smarte DC-DC Technologie für effizientes Batteriemanagement</td>\n               <td class=\"with-image\"> </td>\n              <td class=\"with-image\"> </td>\n            </tr>\n                        <tr >\n                <td>99% kompatibel mit dem aktuellen Mikroinverter</td>\n                <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n\n                                    <tr >\n                <td>Bypass Modus bei vollem Akku</td>\n                <td class=\"with-image\"> </td>\n               <td class=\"with-image\"> </td>\n            </tr>\n    </table>\n    <div id=\"controls_pc\" class=\"controls_pc\" style=\"width:100%;text-align:center;\">\n        <a href=\"javascript:void(0)\"   onclick=\"showAll()\">weitere Infos &darr;</a>\n        <a href=\"javascript:void(0)\" style=\"display:none;\"  onclick=\"showPartial()\">weniger anzeigen &uarr;</a>\n    </div>\n</div>\n    <script>\n        function showAll() {\n           const rows = document.querySelectorAll('#myTable tr');\n            rows.forEach((row, index) => {\n                 row.style.display = '';\n            });\n            const arows = document.querySelectorAll('#controls_pc a');\n            arows.forEach((arow, index) => {\n                if(index==0){\n                    arow.style.display = 'none';\n                }else{\n                    arow.style.display = '';\n                }\n                 \n            });\n\n        }\n\n        function showPartial() {\n       \n            const rows = document.querySelectorAll('#myTable tr');\n            rows.forEach((row, index) => {\n                if (index >= 4) { // 只显示前2行，其余隐藏\n                    row.style.display = 'none';\n                } else {\n                    row.style.display = '';\n                }\n            });\n\n        const arows = document.querySelectorAll('#controls_pc a');\n            arows.forEach((arow, index) => {\n                if(index==0){\n                    arow.style.display = '';\n                }else{\n                    arow.style.display = 'none';\n                }\n                 \n            });\n        }\n\n        // 初始状态显示部分行\n        document.addEventListener('DOMContentLoaded', showPartial);\n    </script>",
