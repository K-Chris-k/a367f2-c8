    font-family: 'Inter';
    line-height: 22px;
    font-size: 18px;
    color: #666;
  }
  
  #shopify-section-{{ section.id }} .details-box {
    gap: 168px;
  }
  
  #shopify-section-{{ section.id }} .details-box .item {
    display: flex;
    flex-flow: column;
  }
  
  #shopify-section-{{ section.id }} .details-box strong {
    line-height: 44px;
    font-weight: 700;
    font-size: 36px;
    background: linear-gradient(180deg, #3250C3 0%, #91AFFA 100%);
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
  }
  
  #shopify-section-{{ section.id }} .details-box span {
    line-height: 19px;
    font-size: 16px;
  }
