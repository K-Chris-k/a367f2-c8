{%- liquid
  assign desktop_height = section.settings.desktop_height
  assign mobile_height = section.settings.mobile_height
-%}
<style>


  {% comment %} #shopify-section-{{ section.id }}{padding-top:{{section.settings.padding_top  }}px;padding-bottom:{{section.settings.padding_bottom  }}px;} {% endcomment %}

  
  #shopify-section-{{ section.id }} {
      --section-padding-top: {{ section.settings.padding_top }}px;
    --section-padding-bottom: {{ section.settings.padding_bottom }}px;  
    {%- if section.settings.image != blank and desktop_height == 'adapt' %}
      --media-ratio: {{ section.settings.image.aspect_ratio }};
    {% endif -%}
    {%- if section.settings.container == 'full' -%}
      --padding-x: var(--page-padding);
    {%- endif -%}
  }
  #shopify-section-{{ section.id }} p{ color:#fff}
  {%- if section.settings.image_mobile != blank and mobile_height == 'adapt' %}
    @media screen and (max-width: 767px) {
      #shopify-section-{{ section.id }} {
        --media-ratio-mobile: {{ section.settings.image_mobile.aspect_ratio }};
      }
    }
  {% endif -%}


    #shopify-section-{{ section.id }} .hd2 { line-height: 56px;font-size: 46px;}
    #shopify-section-{{ section.id }} .hd2  strong{ color: #FFA81D;}
    #shopify-section-{{ section.id }} .rich-text__text { margin-top:14px; line-height: 22px;font-size: 18px;}
    #shopify-section-{{ section.id }} .rich-text__text p { margin:0px; color:inherit}
    #shopify-section-{{ section.id }} .btn-wrapper{display:flex; gap:30px; flex-wrap:wrap;justify-content:{{section.settings.btn_wrapper_alignment }}}
    #shopify-section-{{ section.id }} .white--btn--secondary,#shopify-section-{{ section.id }} .white--btn--secondary:before, 
    #shopify-section-{{ section.id }} .white--btn--secondary:hover:before{ transition: none;background:none;border:2px solid #fff}
    #shopify-section-{{ section.id }} .icon-list ul{ display:flex; flex-wrap:wrap; gap:60px; justify-content: center; margin-top:60px;}
    #shopify-section-{{ section.id }} .icon-list ul li{ display:flex;flex-flow: column; align-items: center; gap:10px;min-width: 94px;max-width: 128px;}
    #shopify-section-{{ section.id }} .icon-list ul li .title{ line-height: 24px; font-size: 16px; color: #FFFFFF; font-weight: 400; }  
  

    
  @media (max-width: 767px){
    {% comment %} #shopify-section-{{ section.id }}{padding-top:{{section.settings.padding_top| divided_by: 2 }}px;padding-bottom:{{section.settings.padding_bottom| divided_by: 2  }}px;} {% endcomment %}
    .image-with-text-ovelay__content-wrapper {
        margin-top: 20px !important;
        margin-left: 10px !important;
        margin-right: 10px !important;
    }
    #shopify-section-{{ section.id }} .icon-list ul{gap:10px; justify-content: center; margin-top:30px;}
    #shopify-section-{{ section.id }} .icon-list ul li {max-width:108px; margin-bottom: 30px;}
    #shopify-section-{{ section.id }} .icon-list ul li img{width:45px;height:45px}
    #shopify-section-{{ section.id }} .icon-list ul li .title{line-height: 16px; font-size: 14px;}
    #shopify-section-{{ section.id }} .content-overlay--middle-left { justify-content: start; top: 2.2rem; }
    #shopify-section-{{ section.id }} .content-overlay--middle-left .z-1{ max-width: 100%; text-align: center; }
    #shopify-section-{{ section.id }} .media--extra-medium{height:60rem}
    #shopify-section-{{ section.id }} .media-wrapper>img{object-fit: fill;}
    #shopify-section-{{ section.id }} .hd2 { line-height: 30px;font-size: 25px;}
    #shopify-section-{{ section.id }} .rich-text__text { margin-top:10px; line-height: 19px;font-size: 16px;}
    #shopify-section-{{ section.id }} .btn-wrapper{ margin-top:3rem;justify-content: center; gap:16px; }
  }

 

</style>
<div class="image-with-text-overlay section--padding" style="background-color: {{ section.settings.bg_color }};">
  <div class="{% if section.settings.container == 'full' %}w-full{% else %}page-width{% endif %}">
    <div class="relative overflow-hidden{% unless section.settings.container == 'full' %} blocks-radius{% endunless %}">
      <parallax-element
        class="block w-full h-full overflow-hidden"
        {% if section.settings.enable_parallax %}
          data-parallax="0.3" data-parallax-direction="{{ section.settings.parallax_direction }}"
        {% endif %}
      >
        {% comment %} <motion-element
          {% comment %} data-motion="zoom-out" {% endcomment %}
          class="block relative overflow-hidden w-full h-full media mb:media--{{ mobile_height }}{% if desktop_height == 'adapt' %} media--adapt{% else %} media--{{ desktop_height }}{% endif %}"
        > {% endcomment %}
          <div class="block relative overflow-hidden w-full h-full media mb:media--{{ mobile_height }}{% if desktop_height == 'adapt' %} media--adapt{% else %} media--{{ desktop_height }}{% endif %}">
          {%- if section.settings.image != blank -%}
            {%- capture sizes -%}{% if section.settings.container == 'full' %}100vw{% else %}min({{ settings.page_width }}px, 100vw){% endif %}{%- endcapture -%}
            <picture class="media-wrapper media--height block w-full h-full relative overflow-hidden">
              {%- if section.settings.image_mobile -%}
                <source
                  media="(max-width: 767px)"
                  srcset="{{ section.settings.image_mobile | image_url: width: '400x' }} 400w, {{ section.settings.image_mobile | image_url: width: '600x' }} 600w, {{ section.settings.image_mobile | image_url: width: '800x' }} 800w, {{ section.settings.image_mobile | image_url: width: '1000x' }} 1000w"
                  width="{{ section.settings.image_mobile.width }}"
                  height="{{ section.settings.image_mobile.height }}"
                >
              {%- endif -%}

              {{-
                section.settings.image
                | image_url: width: section.settings.image.width
                | image_tag:
                  loading: 'lazy',
                  sizes: sizes,
                  widths: '300,400,500,600,700,800,900,1000,1200,1400,1600,1800,2000,2200,2400,2600,2800,3000,3200',
                  is: 'image-lazy'
              -}}
            </picture>
          {%- else -%}
            <div class="media-wrapper">
              {{- 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg' -}}
            </div>
          {%- endif -%}
          </div>
        {% comment %} </motion-element> {% endcomment %}
      </parallax-element>
      <div class="image-with-text-overlay__content">
        <div class="image-with-text-overlay__content-wrapper content-overlay section--padding content-overlay--{{ section.settings.content_position }} color-{{ section.settings.color_scheme }}{% if section.settings.container == 'full' %} page-width{% endif %}">
          <div class="relative show-up-div z-1 rich-text text-{{ section.settings.content_alignment }}" style="max-width: {{ section.settings.max_width }}px;">
            {%- assign motion_delay = 50 -%}
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'subheading' %}
                  <motion-element
                    data-motion="fade-up"
                    data-motion-delay="{{ motion_delay }}"
                    class="block rich-text__subheading text-subheading"
                    {{ block.shopify_attributes }}
                  >
                    {{ block.settings.subheading }}
                  </motion-element>
                  {%- assign motion_delay = motion_delay | plus: 50 -%}
                {% when 'heading' %}
                  <h2 class="rich-text__heading {{ block.settings.heading_size }}" style="color:{{ section.settings.title_color }}" {{ block.shopify_attributes }} >
                    <motion-element data-motion="fade-up" data-motion-delay="{{ motion_delay }}" class="block">
                      {% render 'highlight-text',
                        text: block.settings.heading,
                        style: block.settings.heading_highlight_style
                      %}
                    </motion-element>
                  </h2>
                  {%- assign motion_delay = motion_delay | plus: 50 -%}
                {% when 'text' %}
                  <motion-element
                    data-motion="fade-up"
                    data-motion-delay="{{ motion_delay }}"
                    class="block rich-text__text rte {{ block.settings.text_size }}"
                    style="color:{{ section.settings.text_color }}"
                    {{ block.shopify_attributes }}
                  >
                    {{- block.settings.text -}}
                  </motion-element>
                  {%- assign motion_delay = motion_delay | plus: 50 -%}
                {% when 'button' %}
                  <motion-element class="block flex btn-wrapper" data-motion="fade-up" data-motion-delay="{{ motion_delay }}">
                    {% if block.settings.button_label_first != blank %}
                      <a
                        {% if block.settings.button_link_first != blank %}
                          href="{{ block.settings.button_link_first }}"
                        {% else %}
                          role="link" aria-disabled="true"
                        {% endif %}
                        class="btn {{ block.settings.button_style_first }}"
                        {{ block.shopify_attributes }}
                      >
                        {{- block.settings.button_label_first -}}
                      </a>
                    {% endif %}
                    {% if block.settings.button_label_second != blank %}
                       <a
                        {% if block.settings.button_link_second != blank %}
                          href="{{ block.settings.button_link_second }}"
                        {% else %}
                          role="link" aria-disabled="true"
                        {% endif %}
                        class="btn {{ block.settings.button_style_second }}"
                        {{ block.shopify_attributes }}
                      >
                        {{- block.settings.button_label_second -}}
                      </a>
                      {% endif %}
                  </motion-element>
                {%- assign motion_delay = motion_delay | plus: 50 -%}
                {% when 'icon_list' %}
                  <div class="icon-list">
                  <motion-element class="block" data-motion="fade-up" data-motion-delay="{{ motion_delay }}">
                    <ul>
                        {% for i in (1..6) %}
                        {% capture img_key %}image{{ i }}{% endcapture %}
                        {% capture title_key %}title{{ i }}{% endcapture %}
                        {% assign image = block.settings[img_key] %}
                        {% assign title = block.settings[title_key] %}
                        {% if image or title %}
                          <li>
                            {% if image %}
                              <img src="{{ image | img_url: '120x' }}" alt="{{ title }}">
                            {% endif %}
                            {% if title %}
                              <div class="title">{{ title }}</div>
                            {% endif %}
                          </li>
                        {% endif %}
                      {% endfor %}
                    </ul>
                  </motion-element>
                  </div>
                {% when 'countdown' %}
                  {% render 'kv-countdown',block:block %}
                 
              {% endcase %}
            {% endfor %}
          </div>
        </div>
      </div>
      <div
        class="bg-overlay absolute inset-0"
        style="--color-overlay-alpha: {{ section.settings.overlay_opacity }};"
      ></div>
    </div>
  </div>
</div>

<script>
// 最简单的实现
document.addEventListener('DOMContentLoaded', function() {
  // 添加CSS
const css = `
  .show-up-on-scroll {
    opacity: 0;
    transform: scale(1.4);
    filter: blur(5px); /* 初始模糊效果 */
    transition: all 2.7s cubic-bezier(0.34, 1.56, 0.64, 1);
  }
  
  .show-up-on-scroll.show-up-visible {
    opacity: 1;
    transform: scale(1);
    filter: blur(0); /* 动画结束时消除模糊 */
  }
`;


 
  
  const style = document.createElement('style');
  style.textContent = css;
  document.head.appendChild(style);
  
  // 初始化观察者
  const observer = new IntersectionObserver((entries) => {
    entries.forEach(entry => {
      if (entry.isIntersecting) {
        entry.target.classList.add('show-up-visible');
      } else {
        // 如果需要每次出现都动画，就移除类
        entry.target.classList.remove('show-up-visible');
      }
    });
  }, { threshold: 0.1 });
  
  // 为所有需要动画的元素添加类并观察
  document.querySelectorAll('.show-up-div').forEach(el => {
    el.classList.add('show-up-on-scroll');
    observer.observe(el);
  });
});


</script>
